'scopeName': 'source.mifare-mfd'
'name': 'MifareDump'
'fileTypes': ['mfd']

'patterns': [
    {
        'match': '^:([0-9a-fA-F]{12})([0-9a-fA-F]{8})([0-9a-fA-F]{12})$'
        'captures':
            '1':
                'name': 'constant.keyA.mifare-mfd'
            '2':
                'name': 'string.accessbytes.mifare-mfd'
            '3':
                'name': 'constant.keyB.mifare-mfd'
    }
]
